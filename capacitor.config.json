{
    "appId": "io.kawaii.doodle",
    "appName": "Kawaii Doodle",
    "webDir": "www",
    "server": {
        "androidScheme": "https"
    },
    "plugins": {
        "GoogleAuth": {
            "scopes": [
                "profile",
                "email"
            ],
            "serverClientId": "REPLACE_WITH_YOUR_WEB_CLIENT_ID",
            "forceCodeForRefreshToken": true
        }
    }
}